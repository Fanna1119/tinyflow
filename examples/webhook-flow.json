{
  "id": "webhook-example",
  "name": "Webhook to API Example",
  "description": "Simulates webhook data processing",
  "version": "1.0.0",
  "nodes": [
    {
      "id": "webhook-trigger",
      "functionId": "core.start",
      "params": {
        "input": {"message": "Webhook received", "timestamp": "2024-01-01T00:00:00Z"}
      },
      "position": { "x": 100, "y": 150 }
    },
    {
      "id": "log-webhook",
      "functionId": "core.log",
      "params": {
        "message": "Webhook data received"
      },
      "position": { "x": 350, "y": 150 }
    },
    {
      "id": "process",
      "functionId": "core.setValue",
      "params": {
        "key": "result",
        "value": "Webhook processed successfully"
      },
      "position": { "x": 600, "y": 150 }
    },
    {
      "id": "end",
      "functionId": "core.end",
      "params": {
        "outputKey": "result"
      },
      "position": { "x": 850, "y": 150 }
    }
  ],
  "edges": [
    { "from": "webhook-trigger", "to": "log-webhook", "action": "default" },
    { "from": "log-webhook", "to": "process", "action": "default" },
    { "from": "process", "to": "end", "action": "default" }
  ],
  "flow": {
    "startNodeId": "webhook-trigger"
  }
}
